<div class="panel panel-primary logs">
  <div class="panel-heading panel-heading-logs">
    <button type="button" class="btn btn-default btn-xs logs-clear"><i class="fa fa-trash"></i> Clear</button>
    <button type="button" class="btn btn-default btn-xs logs-time active"><i class="fa fa-calendar"></i> Time</button>
    <button type="button" class="btn btn-default btn-xs logs-pin active"><i class="fa fa-paperclip"></i> Pin</button>
    <h4 class="panel-title">Logs</h4>
  </div>
  <div class="panel-body panel-logs">
    <ul class="logs" data-hook="logs"></ul>
  </div>
</div>
