{
  "name": "boss-web",
  "author": "Alex Potsides",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install"
  },
  "prefer-global": true,
  "bin": {
    "bs-web": "bin/bs-web",
    "boss-web": "bin/bs-web"
  },
  "dependencies": {
    "ampersand-dom": "^1.0",
    "ampersand-form-view": "^2.0",
    "ampersand-input-view": "^3.0",
    "ampersand-model": "^4.0",
    "ampersand-collection": "^1.0",
    "ampersand-rest-collection": "^2.0",
    "ampersand-router": "^1.0",
    "ampersand-view": "^7.0",
    "ampersand-view-switcher": "^1.0",
    "ampersand-collection-view": "^1.0",
    "andlog": "^1.0",
    "async": "^0.9",
    "clientconfig": "^1.0",
    "domify": "^1.0",
    "domready": "^1.0",
    "favicon-setter": "^0.1",
    "getconfig": "^0.3",
    "hapi": "^7.0",
    "hapi-session": "^0.1",
    "hapi-auth-basic": "^1.1",
    "moonboots_hapi": "^3.0",
    "stylizer": "^2.0",
    "templatizer-hbs": "^0.0",
    "underscore": "^1.0",
    "handlebars": "^2.0",
    "semver": "^4.0",
    "moment": "^2.0",
    "browserify-zepto": "^1.1.4",
    "highcharts-browserify": "^0.1.1-4.0.3",
    "browserify-shim": "^3.0",
    "bower": "^1.0",
    "wantsit": "^1.0",
    "ws": "^0.4",
    "wildemitter": "^1.0",
    "columbo": "^1.0",
    "prettysize": "^0.0",
    "ansi-to-html": "^0.2",
    "rc": "^0.5",
    "pem": "^1.0",
    "commander": "^2.0",
    "prompt": "^0.2",
    "colors": "^1.0",
    "bcrypt": "^0.8"
  },
  "devDependencies": {},
  "jshintConfig": {
    "asi": false,
    "browser": true,
    "curly": false,
    "expr": true,
    "indent": 4,
    "loopfunc": true,
    "node": true,
    "trailing": true,
    "undef": true,
    "white": true
  },

  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "Highcharts": {
      "depends": ["HighchartsAdapter:HighchartsAdapter"],
      "exports": "Highcharts"
    },
    "HighchartsMore": {
      "depends": ["HighchartsAdapter:HighchartsAdapter", "Highcharts:Highcharts"],
      "exports": "HighchartsMore"
    },
    "HighchartsAdapter": {
      "exports":"HighchartsAdapter"
    },
    "HighchartsSolidGauge": {
      "depends": ["HighchartsAdapter:HighchartsAdapter", "Highcharts:Highcharts", "HighchartsMore:HighchartsMore"],
      "exports": "HighchartsSolidGauge"
    }
  },
  "browser": {
    "Highcharts": "./bower_components/highcharts-release/highcharts.src.js",
    "HighchartsMore": "./bower_components/highcharts-release/highcharts-more.src.js",
    "HighchartsAdapter": "./bower_components/highcharts-release/adapters/standalone-framework.src.js",
    "HighchartsSolidGauge": "./bower_components/highcharts-release/modules/solid-gauge.src.js"
  }
}